{
  "$schema": "../SampleWebApplication/JsonSchemas/rerabbit.schema.json",
  "RabbitMq": {
    "PublisherConnections": {
      "DefaultConnection": {
        "VirtualHosts": {
          "/": {
            "UserName": "user",
            "Password": "user",
            "Messages": {
              "MyIntegrationRabbitMessage": {
                "Version": "v1",
                "Exchange": {
                  "Name": "test-exchange",
                  "Type": "direct",
                  "Durable": true
                },
                "Route": "rk1"
              },
              "MetricsRabbitMessage": {
                "Version": "v1",
                "Exchange": {
                  "Name": "metrics",
                  "Type": "fanout",
                  "Durable": true
                }
              }
            }
          }
        }
      }
    }
  }
}